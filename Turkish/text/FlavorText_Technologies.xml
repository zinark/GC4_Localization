  <FlavorTextDef>
    <InternalName>Tech_XenoBiology_FlavorText_ShortDescription</InternalName>
    <FlavorTextOption>
      <Requirements>
        <SpeakerRaceTrait>SyntheticLife</SpeakerRaceTrait>
      </Requirements>
      <Text>Biyolojik Sistem Entegrasyonu</Text>
